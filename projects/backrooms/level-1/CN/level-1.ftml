</style>

    <div class="buttons" style="text-align: left;">
    <button class="page-rate-widget-box" id="playButton">播放环境音</button>
    </div>
    <!-- Markers for each audio switch point -->
    <div class="marker" style="top: 0%;" data-audio="0"></div>
    <div class="marker" style="top: 29%;" data-audio="1"></div>
    <div class="marker" style="top: 53%;" data-audio="2"></div>
    <div class="marker" style="top: 77%;" data-audio="3"></div>
    <div class="marker" style="top: 86%;" data-audio="4"></div>

    <audio id="audio0" preload="auto" loop>
        <source src="https://lite.imagebed.pro/Level%201%20-%20Habitable%20Zone.mp3" type="audio/mpeg">
    </audio>
    <audio id="audio1" preload="auto" loop>
        <source src="https://lite.imagebed.pro/Level%201%20-%20Storage%20Hall.mp4" type="audio/mpeg">
    </audio>
    <audio id="audio2" preload="auto" loop>
        <source src="https://lite.imagebed.pro/Level%201%20-%20B.T.N.G%20Trader%E2%80%99s%20Keep.mp4" type="audio/mpeg">
    </audio>
    <audio id="audio3" preload="auto" loop>
        <source src="https://lite.imagebed.pro/Level%201%20-%20M.E.G%20Alpha%20Base.mp4" type="audio/mpeg">
    </audio>
    <audio id="audio4" preload="auto" loop>
        <source src="https://lite.imagebed.pro/Level%202%20-%20The%20Survivors.mp4" type="audio/mpeg">
    </audio>

    <script>
        const playButton = document.getElementById('playButton');
        const audios = [
            document.getElementById('audio0'),
            document.getElementById('audio1'),
            document.getElementById('audio2'),
            document.getElementById('audio3'),
            document.getElementById('audio4')
        ];
        const markers = document.querySelectorAll('.marker');
        let isPlaying = false;
        let currentAudioIndex = -1;
        const fadeDuration = 1000; // Fade duration in milliseconds
        let isSwitching = false; // Prevent multiple switches at once

        // Start audio on button click
        playButton.addEventListener('click', () => {
            if (!isPlaying) {
                fadeIn(audios[0]);
                isPlaying = true;
                currentAudioIndex = 0;
                playButton.textContent = '播放中...';
            }
        });

        // Fade out an audio and return a Promise
        function fadeOut(audio) {
            return new Promise((resolve) => {
                if (!audio.paused && audio.volume > 0) {
                    const initialVolume = audio.volume;
                    const fadeStep = initialVolume / (fadeDuration / 50);
                    const fade = setInterval(() => {
                        if (audio.volume > fadeStep) {
                            audio.volume -= fadeStep;
                        } else {
                            audio.volume = 0;
                            audio.pause();
                            audio.currentTime = 0;
                            clearInterval(fade);
                            resolve();
                        }
                    }, 50);
                } else {
                    audio.pause();
                    audio.currentTime = 0;
                    audio.volume = 0;
                    resolve();
                }
            });
        }

        // Fade in an audio
        function fadeIn(audio) {
            audio.volume = 0;
            audio.play();
            const fadeStep = 1 / (fadeDuration / 50);
            const fade = setInterval(() => {
                if (audio.volume < 1 - fadeStep) {
                    audio.volume += fadeStep;
                } else {
                    audio.volume = 1;
                    clearInterval(fade);
                }
            }, 50);
        }

        // Switch audio with sequential fade-out then fade-in
        async function switchAudio(newIndex) {
            if (newIndex === currentAudioIndex || newIndex < 0 || newIndex >= audios.length || isSwitching) return;

            isSwitching = true;
            const oldAudio = currentAudioIndex >= 0 ? audios[currentAudioIndex] : null;
            const newAudio = audios[newIndex];

            if (oldAudio) {
                await fadeOut(oldAudio); // Wait for fade-out to complete
            }
            fadeIn(newAudio);
            currentAudioIndex = newIndex;
            isSwitching = false;
        }

        // Intersection Observer to detect scroll position
        const observer = new IntersectionObserver((entries) => {
            if (!isPlaying) return;

            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    const audioIndex = parseInt(entry.target.dataset.audio);
                    switchAudio(audioIndex);
                }
            });
        }, { threshold: 0 });

        // Observe all markers
        markers.forEach(marker => observer.observe(marker));

        // Cleanup
        window.addEventListener('unload', () => {
            audios.forEach(audio => {
                audio.pause();
                audio.volume = 0;
            });
            observer.disconnect();
        });

    </script>
